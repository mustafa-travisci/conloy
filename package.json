{
  "name": "conloy",
  "version": "1.1.9",
  "description": "A lightweight solution to the tedious process of writing if return statements.",
  "main": "convert.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElitKrit/conloy"
  },
  "keywords": [
    "convert",
    "if",
    "return",
    "conversion"
  ],
  "author": "Krit Dass",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ElitKrit/conloy/issues"
  },
  "homepage": "https://github.com/ElitKrit/conloy#readme",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0"
  }
}
